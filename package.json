{
	"name": "@egjs/view360",
	"version": "4.0.0",
	"description": "360 integrated viewing solution from inside-out view to outside-in view. It provides user-friendly service by rotating 360 degrees through various user interaction such as motion sensor and touch.",
	"main": "dist/view360.js",
	"module": "dist/view360.esm.js",
	"es2015": "dist/view360.esm.js",
	"types": "declaration/index.d.ts",
	"sideEffects": [
		"**/*.css",
		"**/*.sass"
	],
	"scripts": {
		"start": "rollup -c ./rollup.config.dev.js -w",
		"build": "rm -rf ./dist && rollup -c && npm run declaration && npm run printsizes",
		"declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
		"printsizes": "print-sizes ./dist --exclude=\\.map",
		"test": "karma start",
		"test:chrome": "karma start --chrome",
		"lint": "eslint 'src/**/*.ts'",
		"docs": "typedoc --options typedoc.json && ts-node -O '{\"module\":\"commonjs\"}' docgen/generate.ts",
		"coverage": "karma start --coverage",
		"coverage:nogl": "karma start --coverage --nogl",
		"release": "release-helper upstream"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/naver/egjs-view360"
	},
	"author": {
		"name": "NAVER Corp."
	},
	"namespace": {
		"eg": "eg"
	},
	"license": "MIT",
	"homepage": "https://github.com/naver/egjs-view360",
	"bugs": {
		"url": "https://github.com/naver/egjs-view360/issues"
	},
	"browserslist": [
		"last 2 chrome versions",
		"ie 11",
		"not op_mini all"
	],
	"keywords": [
		"360-photo",
		"360-video",
		"view360",
		"panorama",
		"spinner",
		"webgl",
		"device-orientation",
		"touch",
		"VR",
		"3D",
		"canvas",
		"360"
	],
	"devDependencies": {
		"@egjs/release-helper": "0.0.3",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^11.1.0",
		"@rollup/plugin-node-resolve": "^7.1.3",
		"@rollup/plugin-replace": "^4.0.0",
		"@types/karma-chai": "^0.1.2",
		"@types/mocha": "^8.0.3",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/eslint-plugin-tslint": "^4.18.0",
		"@typescript-eslint/parser": "^4.18.0",
		"chai": "^4.1.2",
		"cpx": "^1.5.0",
		"eslint": "^7.22.0",
		"eslint-plugin-compat": "^4.0.2",
		"eslint-plugin-es": "^4.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^30.7.9",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"fs-extra": "^7.0.0",
		"gh-pages": "^1.2.0",
		"husky": "^0.14.3",
		"karma": "^5.2.3",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^3.1.0",
		"karma-coverage-istanbul-reporter": "^2.0.1",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.5",
		"karma-rollup-preprocessor": "^7.0.5",
		"karma-sinon": "^1.0.5",
		"karma-sourcemap-loader": "^0.3.7",
		"karma-typescript": "^5.2.0",
		"karma-webpack": "^3.0.5",
		"mocha": "^5.2.0",
		"print-sizes": "0.0.3",
		"rollup": "^1.32.1",
		"rollup-plugin-glslify": "^1.2.1",
		"rollup-plugin-livereload": "^1.3.0",
		"rollup-plugin-prototype-minify": "^1.1.0",
		"rollup-plugin-serve": "^1.0.1",
		"rollup-plugin-typescript2": "^0.27.0",
		"rollup-plugin-uglify": "^6.0.4",
		"rollup-plugin-visualizer": "^4.0.4",
		"sinon": "^6.3.3",
		"ts-node": "^10.9.1",
		"typedoc": "^0.23.10",
		"typescript": "^4.7.4"
	},
	"husky": {
		"hooks": {
			"commit-msg": "node ./config/validate-commit-msg.js"
		}
	},
	"dependencies": {
		"@egjs/component": "^3.0.2",
		"@egjs/imready": "^1.3.0",
		"gl-matrix": "^3.4.3"
	}
}
