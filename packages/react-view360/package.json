{
  "name": "@egjs/react-view360",
  "version": "4.0.0",
  "description": "360 integrated viewing solution from inside-out view to outside-in view. It provides user-friendly service by rotating 360 degrees through various user interaction such as motion sensor and touch.",
  "main": "dist/view360.cjs.js",
  "module": "dist/view360.esm.js",
  "sideEffects": [
    "**/*.css",
    "**/*.sass"
  ],
  "es2015": "dist/view360.esm.js",
  "types": "declaration/index.d.ts",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "rollup -c && npm run declaration",
    "postbuild": "cpx 'node_modules/@egjs/view360/css/*' css/ && cpx 'node_modules/@egjs/view360/sass/*' sass/",
    "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@egjs/view360": "~4.0.0"
  },
  "devDependencies": {
    "@egjs/build-helper": "0.0.5",
    "@types/jest": "^29.4.0",
    "@types/node": "12.0.0",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/react-is": "^17.0.0",
    "@types/react-router-dom": "^5.3.3",
    "cpx": "^1.5.0",
    "customize-cra": "^1.0.0",
    "react": "16.8.0",
    "react-app-rewire-alias": "^1.1.7",
    "react-app-rewired": "^2.2.1",
    "react-dom": "16.8.0",
    "react-router-dom": "^5.3.3",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "16.8.0",
    "rollup": "^2.45.2",
    "jest": "^29.4.1",
    "jsdom": "^21.1.0",
    "tslib": "^2.2.0",
    "ts-node": "^10.9.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naver/egjs-view360/tree/master/packages/react-view360"
  },
  "author": {
    "name": "NAVER Corp."
  },
  "namespace": {
    "eg": "eg"
  },
  "homepage": "https://naver.github.io/egjs-view360",
  "bugs": {
    "url": "https://github.com/naver/egjs-view360/issues"
  },
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
